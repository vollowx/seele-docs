{
  "name": "seele-docs",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node docs-web/build-ssr.js && node docs-web/build-client.js && npx @11ty/eleventy --config=docs-web/eleventy.config.js",
    "dev": "npx @11ty/eleventy --config=docs-web/eleventy.config.js && npm run serve",
    "serve": "wds --watch --root-dir docs-web/_site --open",
    "preview": "wds --root-dir docs-web/_site"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@lit-labs/eleventy-plugin-lit": "^1.0.6",
    "@lit-labs/ssr": "^4.0.0",
    "@lit-labs/ssr-client": "^1.1.8",
    "@types/bun": "latest",
    "@web/dev-server": "^0.4.6",
    "@webcomponents/template-shadowroot": "^0.2.1",
    "esbuild": "^0.27.2",
    "html-minifier": "^4.0.0",
    "tsx": "^4.21.0"
  },
  "dependencies": {
    "@vollowx/seele": "^0.8.8",
    "glob": "^13.0.0",
    "lightningcss": "^1.30.2"
  }
}
