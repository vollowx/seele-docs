    "@types/bun": "latest",
    "@web/dev-server": "^0.4.6",
    "@webcomponents/template-shadowroot": "^0.2.1",
    "esbuild": "^0.27.2",
    "html-minifier": "^4.0.0",
    "tsx": "^4.21.0"
  },
  "dependencies": {
    "@vollowx/seele": "^0.8.2",
    "glob": "^13.0.0",
    "lightningcss": "^1.30.2"
